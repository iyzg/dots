# Jumpapp
"jumpapp urxvt"
  F1

"jumpapp firefox"
  F2

"jumpapp zathura"
  F3

# Multimedia
"amixer sset Master 2%+"
  XF86AudioRaiseVolume

"amixer sset Master 2%-"
  XF86AudioLowerVolume

"amixer sset Master toggle"
  XF86AudioMute

"sudo light -A 2"
  XF86MonBrightnessUp

"sudo light -U 2"
  XF86MonBrightnessDown
