# General
"sudo shutdown now"
  KP_Subtract

# Jumpapp
"jumpapp urxvt"
  KP_End

"jumpapp firefox"
  KP_Down

"jumpapp zathura"
  KP_Next

"jumpapp anki"
  KP_Left

# Multimedia
"amixer sset Master 2%+"
  XF86AudioRaiseVolume

"amixer sset Master 2%-"
  XF86AudioLowerVolume

"amixer sset Master toggle"
  XF86AudioMute

"sudo light -A 2"
  XF86MonBrightnessUp

"sudo light -U 2"
  XF86MonBrightnessDown
